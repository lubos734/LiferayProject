<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<portlet-name>DivisionOverview</portlet-name>
		<display-name>Division Overview</display-name>
		<portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>
		<init-param>
			<name>contextConfigLocation</name>
			<value>/WEB-INF/spring-context/portlet/division/DivisionOverview-portlet.xml</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
		</supports>
		<supported-locale>cs</supported-locale>
		<supported-locale>en</supported-locale>
		<portlet-info>
			<title>Division Overview</title>
			<short-title>Division Overview</short-title>
			<keywords>Division Overview</keywords>
		</portlet-info>
	</portlet>
	<portlet>
		<portlet-name>DivisionCreate</portlet-name>
		<display-name>Division Create</display-name>
		<portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>
		<init-param>
			<name>contextConfigLocation</name>
			<value>/WEB-INF/spring-context/portlet/division/DivisionCreate-portlet.xml</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
		</supports>
		<supported-locale>cs</supported-locale>
		<supported-locale>en</supported-locale>
		<portlet-info>
			<title>Division Create</title>
			<short-title>Division Create</short-title>
			<keywords>Division Create</keywords>
		</portlet-info>
	</portlet>
</portlet-app>